did 2 coding questions based on array : 

1. two sum 

brute force
class Solution{
    public int[] twoSum(int[] nums, int target) {
        int[] arr = new int[2];
        for (int i=0 ; i<nums.length-1 ; i++){
            for (int j=i+1 ; j<nums.length ; j++){
                if((nums[i]+nums[j])==target){
                    arr[0]=i;
                    arr[1]=j;
                }
            }
        }
        return arr;
    }
}


/* class Solution{
    public int[] twoSum(int[] nums, int target) {
        for (int i=0 ; i<nums.length-1 ; i++){
            for (int j=i+1 ; j<nums.length ; j++){
                if((nums[i]+nums[j])==target){
                    return new int[] {i,j};
                }
            }
        }
        return null;
    }
}
